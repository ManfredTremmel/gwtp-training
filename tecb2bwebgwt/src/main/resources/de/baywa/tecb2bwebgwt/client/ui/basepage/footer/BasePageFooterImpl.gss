.footer {
  width: 100%;
  height: 30px;
  float: left;
  border-top: 1px solid #d0d789;
  margin: 15px 0 0 0;
}
.footer div, .footer a {
  float: left;
  position: relative;
  font-size: 11px;
  text-decoration: none;
  color: #6c7600;
  font-weight: bold;
  padding-left: 10px;
  cursor: pointer;
}
.footer a:hover {
  text-decoration: underline;
  color: #6c7600;
}
.print {
  float: right !important;
  padding-right: 10px;
}
